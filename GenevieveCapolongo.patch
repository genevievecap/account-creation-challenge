+    ${USERNAME_NOT_VALID} | ${PASSWORD_LETTER_NUMBER_MISSING} | ${'validusername'} | ${'aaaaaaaaaaaaaaaaaaaaaaa'}
+    ${SERVER_ERROR}       | ${PASSWORD_NOT_VALID}             | ${'short'}         | ${''}
